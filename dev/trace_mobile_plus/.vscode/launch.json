    "version": "0.2.0",
    "configurations": [
        {
            "name": "trace_mobile_plus",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "-d",
                "chrome"
            ]
        },
        {
            "name": "trace_mobile_plus (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "-d",
                "edge"
            ]
        },
        {
            "name": "trace_mobile_plus (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "preLaunchTask": "cleanup-before-launch",
            "args": [
                "-d",
                "edge"
            ]
        }
    ]
}